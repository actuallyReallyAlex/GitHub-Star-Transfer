{
  "name": "github-star-transfer",
  "version": "0.1.0",
  "description": "A simple CLI app written with Puppeteer and NodeJS to manually re-star repositories in GitHub from one account to another.",
  "main": "index.js",
  "bin": {
    "star-transfer": "./index.js"
  },
  "scripts": {
    "document": "documentation build lib/helpers.js index.js -f html -o docs --sort-order alpha --config documentation.yml -g",
    "delete-config": "trash /Users/alex/.config/configstore/github-star-transfer.json",
    "update-changelog": "github_changelog_generator -u alexlee-dev -p GitHub-Star-Transfer",
    "record-gif": "echo \"svg-term --cast=CAST_ID --out FILENAME.svg --window\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexlee-dev/GitHub-Star-Transfer.git"
  },
  "keywords": [
    "cli",
    "cli-app",
    "puppeteer",
    "github",
    "star",
    "transfer"
  ],
  "author": "Alex Lee",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/alexlee-dev/GitHub-Star-Transfer/issues"
  },
  "homepage": "https://github.com/alexlee-dev/GitHub-Star-Transfer/",
  "dependencies": {
    "boxen": "^1.3.0",
    "chalk": "^2.4.1",
    "clear": "^0.1.0",
    "configstore": "^4.0.0",
    "dotenv": "^6.0.0",
    "figlet": "^1.2.0",
    "gradient-string": "^1.1.0",
    "inquirer": "^6.1.0",
    "is-online": "^7.0.0",
    "ora": "^3.0.0",
    "pretty-error": "^2.1.1",
    "puppeteer": "^1.7.0",
    "request": "^2.88.0",
    "write-data": "^1.0.2"
  },
  "devDependencies": {
    "eslint": "^5.4.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^3.1.0"
  }
}
